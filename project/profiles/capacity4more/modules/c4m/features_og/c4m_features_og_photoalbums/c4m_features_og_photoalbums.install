<?php
/**
 * @file
 * Installation hooks for the Group Photoalbums feature.
 */

/**
 * Implements hook_install().
 */
function c4m_features_og_photoalbums_install() {
  // Install this feature.
  c4m_og_feature_install('c4m_features_og_photoalbums');
}

/**
 * Implements hook_uninstall().
 */
function c4m_features_og_photoalbums_uninstall() {
  // Uninstall this feature.
  c4m_og_feature_uninstall('c4m_features_og_photoalbums');
}
