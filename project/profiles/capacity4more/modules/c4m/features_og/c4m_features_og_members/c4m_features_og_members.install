<?php

/**
 * @file
 * Installation hooks for the Group Members feature.
 */

/**
 * Implements hook_install().
 */
function c4m_features_og_members_install() {
  // Install this feature.
  c4m_og_feature_install('c4m_features_og_members');
}

/**
 * Implements hook_uninstall().
 */
function c4m_features_og_members_uninstall() {
  // Uninstall this feature.
  c4m_og_feature_uninstall('c4m_features_og_members');
}
